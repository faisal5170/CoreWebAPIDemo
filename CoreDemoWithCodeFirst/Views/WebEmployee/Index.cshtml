@model List<CoreDemoWithCodeFirst.Models.MyFirstTables>
@{ 
    Layout = "~/Pages/Shared/_Layout.cshtml";
}
<h1>Employee Index</h1>

<!DOCTYPE html>
<html lang="en">
<body>

    <div class="container">

        <table class="table">
            <thead>
                <tr>
                    <th>Firstname</th>
                    <th>Lastname</th>
                    <th>Action</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <td>@item.Name</td>
                        <td>@item.Age</td>
                        <td></td>
                    </tr>
                }
            </tbody>
        </table>
    </div>

</body>
</html>
